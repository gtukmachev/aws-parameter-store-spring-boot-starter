<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{dd-MM-yyyy HH:mm:ss.SSS} %magenta([%thread]) %highlight(%-5level) %magenta(%logger{0}.%M) %msg%n</pattern>
        </encoder>
    </appender>

    <root level="error"> <appender-ref ref="STDOUT" /> </root>

    <logger name="tga.aws.spring.parameterstore" level="info"/>

</configuration>